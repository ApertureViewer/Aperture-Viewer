<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<floater
 height="80"
 help_topic="floater_load_preset"
 layout="topleft"
 name="load_pref_preset"
 save_rect="true"
 title="Load Pref Preset"
 width="288">

    <string name="title_graphic">Load Graphic Preset</string>
    <string name="title_camera">Load Camera Preset</string>

    <panel
     name="P_Load_Preset_Header"
     follows="top|left"
     layout="topleft"
     top_pad="3"
     left="0"
     width="288"
     height="22"
     bg_alpha_color="Phototools_Header"
     border_visible="false"
     border_thickness="1"
     border="none"
     background_visible="true">

        <text
        name="Preset"
         follows="left|top"
         layout="topleft"
         left="7"
         font="SansSerif"
         height="13"
         width="275">
         Select a preset
        </text>

    </panel>

    <panel
     name="P_Panel_Preset_Settings"
     follows="top|left"
     layout="topleft"
     top_pad="0"
     left="0"
     width="288"
     height="57" 
     border_visible="false"
     bevel_style="none"
     background_visible="true">

        <combo_box
         follows="top|left"
         layout="topleft"
         left="5"
         name="preset_combo"
         top_pad="5"
         height="22"
         right="-5"/>

        <button
         name="ok"
         label="OK"
         follows="top|left"
         height="22"
         layout="topleft"
         top_pad="3"
         left="5"
         width="100"/>

        <button
         follows="bottom|right"
         height="22"
         label="Cancel"
         layout="topleft"
         left_pad="0"
         name="cancel"
         width="100"/>

    </panel>

</floater>
